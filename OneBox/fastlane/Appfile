# Fastlane Appfile
# For more information about the Appfile, see:
# https://docs.fastlane.tools/advanced/#appfile

# Your Apple ID
apple_id ENV['FASTLANE_USER'] || "your-apple-id@example.com"

# App Identifier
app_identifier "com.yourcompany.onebox"

# Team ID (find it at https://developer.apple.com/account/#/membership/)
team_id ENV['FASTLANE_TEAM_ID'] || "YOUR_TEAM_ID"

# iTunes Connect Team ID (if different from Developer Portal)
itc_team_id ENV['FASTLANE_ITC_TEAM_ID'] || "YOUR_ITC_TEAM_ID"

# For more information about environment variables, see:
# https://docs.fastlane.tools/advanced/#environment-variables
